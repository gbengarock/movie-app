{
  "name": "movie-app",
  "version": 2,
  "builds": [
    {
      "src": "build", // Path to your build directory
      "use": "@vercel/static-build" // Builder for React apps
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_KEY": "@my-secret-api-key"
    // Add other environment variables here if needed
  },
  "public": true
}
